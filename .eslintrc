{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],

  "rules": {
    "semi": "off",
    "no-unused-vars": "warn",
    "no-unused-expressions": "warn",
    "no-useless-return": "warn"
  },
  "plugins": ["jsx-a11y"],
  "env": {"browser": true}
}
